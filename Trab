            PROC_GRAVAR_LOG('EXCLUSÃO','COMPRA','FOR_CODIGO',:OLD.FOR_CODIGO,NULL);
            PROC_GRAVAR_LOG('EXCLUSÃO','COMPRA','CPR_EMISSAO',:OLD.CPR_EMISSAO,NULL);
            PROC_GRAVAR_LOG('EXCLUSÃO','COMPRA','CPR_VALOR',:OLD.CPR_VALOR,NULL);
            PROC_GRAVAR_LOG('EXCLUSÃO','COMPRA','CPR_DESCONTO',:OLD.CPR_DESCONTO,NULL);
            PROC_GRAVAR_LOG('EXCLUSÃO','COMPRA','CPR_TOTAL',:OLD.CPR_TOTAL,NULL);
            PROC_GRAVAR_LOG('EXCLUSÃO','COMPRA','CPR_ENTRADA',:OLD.CPR_ENTRADA,NULL);
            PROC_GRAVAR_LOG('EXCLUSÃO','COMPRA','CPR_OBS',:OLD.CPR_OBS,NULL);
    END;
    /


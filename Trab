CREATE OR REPLACE TRIGGER TRG_UPDATE_CLIENTE 
    AFTER UPDATE ON  CLIENTE
    FOR EACH ROW
    BEGIN
        IF(:OLD.CLI_CODIGO != :NEW.CLI_CODIGO) THEN
            PROC_GRAVAR_LOG('ALTERACÃO','CLIENTE','CLI_CODIGO',:OLD.CLI_CODIGO,:NEW.CLI_CODIGO); 
            END IF;
        IF(:OLD.PES_CODIGO != :NEW.PES_CODIGO) THEN
            PROC_GRAVAR_LOG('ALTERACÃO','CLIENTE','PES_CODIGO',:OLD.PES_CODIGO,:NEW.PES_CODIGO); 
            END IF;
        IF(:OLD.CLI_LIMITECRED != :NEW.CLI_LIMITECRED) THEN
            PROC_GRAVAR_LOG('ALTERACÃO','CLIENTE','CLI_LIMITECRED',:OLD.CLI_LIMITECRED,:NEW.CLI_LIMITECRED); 
            END IF;
    END;
    /

CREATE OR REPLACE TRIGGER TRG_UPDATE_COMPRA 
    AFTER UPDATE ON  COMPRA
    FOR EACH ROW
    BEGIN
        IF(:OLD.CPR_CODIGO != :NEW.CPR_CODIGO) THEN
            PROC_GRAVAR_LOG('ALTERACÃO','COMPRA','CPR_CODIGO',:OLD.CPR_CODIGO,:NEW.CPR_CODIGO); 
            END IF;
        IF(:OLD.USU_CODIGO != :NEW.USU_CODIGO) THEN

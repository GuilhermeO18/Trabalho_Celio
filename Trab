-- Drops Tabelas --

DROP TABLE "LOG";

-- Drops Sequences --

DROP SEQUENCE SEQ_LOG;


-- Create Tabelas --

CREATE TABLE "LOG" (
    LOG_ID NUMBER(10) NOT NULL,
    LOG_DATA DATE,
    LOG_OPERACAO VARCHAR2(1000),
    LOG_TABELA VARCHAR2(3000),
    LOG_CAMPO VARCHAR2(3000),
    LOG_OLD VARCHAR2(4000),
    LOG_NEW VARCHAR2(4000)
);

-- Create Constraints --

ALTER TABLE "LOG"
    ADD CONSTRAINT pk_log
    PRIMARY KEY(LOG_ID);

-- Create Sequences --

CREATE SEQUENCE SEQ_LOG
    INCREMENT BY 1
    START WITH 1
    NOCYCLE;

CREATE OR REPLACE TRIGGER  TRG_LOG
        BEFORE INSERT ON "LOG"
        FOR EACH ROW
    BEGIN
        SELECT SEQ_LOG.NEXTVAL
        INTO:NEW.LOG_ID
        FROM DUAL;
    END;
/
-- Create Triggers e Procedures --

CREATE OR REPLACE PROCEDURE PROC_GRAVAR_LOG(
    vLOG_OPERACAO IN VARCHAR2, vLOG_TABELA IN VARCHAR2,vLOG_CAMPO IN VARCHAR2,vLOG_OLD IN VARCHAR2,vLOG_NEW IN VARCHAR2)
    IS
    BEGIN
        INSERT INTO "LOG"(LOG_DATA,LOG_OPERACAO, LOG_TABELA,LOG_CAMPO,LOG_OLD,LOG_NEW) VALUES (SYSDATE,vLOG_OPERACAO, vLOG_TABELA,vLOG_CAMPO,vLOG_OLD,vLOG_NEW);
    END PROC_GRAVAR_LOG;
    /

CREATE OR REPLACE TRIGGER TRG_INSERT_CLIENTE
    AFTER INSERT ON  CLIENTE
    FOR EACH ROW
    BEGIN
            PROC_GRAVAR_LOG('INCLUSÃO','CLIENTE','CLI_CODIGO',NULL,:NEW.CLI_CODIGO);
            PROC_GRAVAR_LOG('INCLUSÃO','CLIENTE','PES_CODIGO',NULL,:NEW.PES_CODIGO);
            PROC_GRAVAR_LOG('INCLUSÃO','CLIENTE','CLI_LIMITECRED',NULL,:NEW.CLI_LIMITECRED);
    END;
    /

CREATE OR REPLACE TRIGGER TRG_INSERT_COMPRA
    AFTER INSERT ON  COMPRA
    FOR EACH ROW
    BEGIN
            PROC_GRAVAR_LOG('INCLUSÃO','COMPRA','CPR_CODIGO',NULL,:NEW.CPR_CODIGO);
            PROC_GRAVAR_LOG('INCLUSÃO','COMPRA','USU_CODIGO',NULL,:NEW.USU_CODIGO);
            PROC_GRAVAR_LOG('INCLUSÃO','COMPRA','FOR_CODIGO',NULL,:NEW.FOR_CODIGO);
            PROC_GRAVAR_LOG('INCLUSÃO','COMPRA','CPR_EMISSAO',NULL,:NEW.CPR_EMISSAO);

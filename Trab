            END IF;
        IF(:OLD.FOR_CODIGO != :NEW.FOR_CODIGO) THEN
            PROC_GRAVAR_LOG('ALTERACÃO','COMPRA','FOR_CODIGO',:OLD.FOR_CODIGO,:NEW.FOR_CODIGO); 
            END IF;
        IF(:OLD.CPR_EMISSAO != :NEW.CPR_EMISSAO) THEN

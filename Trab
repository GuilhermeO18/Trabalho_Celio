CREATE OR REPLACE TRIGGER TRG_DELETE_CLIENTE 
    AFTER DELETE ON  CLIENTE
    FOR EACH ROW
    BEGIN
            PROC_GRAVAR_LOG('EXCLUSÃO','CLIENTE','CLI_CODIGO',:OLD.CLI_CODIGO,NULL);
            PROC_GRAVAR_LOG('EXCLUSÃO','CLIENTE','PES_CODIGO',:OLD.PES_CODIGO,NULL);
            PROC_GRAVAR_LOG('EXCLUSÃO','CLIENTE','CLI_LIMITECRED',:OLD.CLI_LIMITECRED,NULL);
    END;
    /


            END IF;
        IF(:OLD.CPR_VALOR != :NEW.CPR_VALOR) THEN
            PROC_GRAVAR_LOG('ALTERACÃO','COMPRA','CPR_VALOR',:OLD.CPR_VALOR,:NEW.CPR_VALOR); 
            END IF;
        IF(:OLD.CPR_DESCONTO != :NEW.CPR_DESCONTO) THEN
            PROC_GRAVAR_LOG('ALTERACÃO','COMPRA','CPR_DESCONTO',:OLD.CPR_DESCONTO,:NEW.CPR_DESCONTO); 
            END IF;
        IF(:OLD.CPR_TOTAL != :NEW.CPR_TOTAL) THEN
            PROC_GRAVAR_LOG('ALTERACÃO','COMPRA','CPR_TOTAL',:OLD.CPR_TOTAL,:NEW.CPR_TOTAL); 
            END IF;
        IF(:OLD.CPR_ENTRADA != :NEW.CPR_ENTRADA) THEN
            PROC_GRAVAR_LOG('ALTERACÃO','COMPRA','CPR_ENTRADA',:OLD.CPR_ENTRADA,:NEW.CPR_ENTRADA); 
            END IF;
        IF(:OLD.CPR_OBS != :NEW.CPR_OBS) THEN

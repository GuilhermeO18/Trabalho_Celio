            PROC_GRAVAR_LOG('ALTERACÃO','VENDA','VDA_CODIGO',:OLD.VDA_CODIGO,:NEW.VDA_CODIGO); 
            END IF;
        IF(:OLD.USU_CODIGO != :NEW.USU_CODIGO) THEN
            PROC_GRAVAR_LOG('ALTERACÃO','VENDA','USU_CODIGO',:OLD.USU_CODIGO,:NEW.USU_CODIGO); 
            END IF;
        IF(:OLD.CLI_CODIGO != :NEW.CLI_CODIGO) THEN
            PROC_GRAVAR_LOG('ALTERACÃO','VENDA','CLI_CODIGO',:OLD.CLI_CODIGO,:NEW.CLI_CODIGO); 
            END IF;
        IF(:OLD.VDA_DATA != :NEW.VDA_DATA) THEN
            PROC_GRAVAR_LOG('ALTERACÃO','VENDA','VDA_DATA',:OLD.VDA_DATA,:NEW.VDA_DATA); 
            END IF;
        IF(:OLD.VDA_VALOR != :NEW.VDA_VALOR) THEN
            PROC_GRAVAR_LOG('ALTERACÃO','VENDA','VDA_VALOR',:OLD.VDA_VALOR,:NEW.VDA_VALOR); 
            END IF;
        IF(:OLD.VDA_DESCONTO != :NEW.VDA_DESCONTO) THEN
            PROC_GRAVAR_LOG('ALTERACÃO','VENDA','VDA_DESCONTO',:OLD.VDA_DESCONTO,:NEW.VDA_DESCONTO); 
            END IF;
        IF(:OLD.VDA_TOTAL != :NEW.VDA_TOTAL) THEN
            PROC_GRAVAR_LOG('ALTERACÃO','VENDA','VDA_TOTAL',:OLD.VDA_TOTAL,:NEW.VDA_TOTAL); 
            END IF;
        IF(:OLD.VDA_OBS != :NEW.VDA_OBS) THEN
            PROC_GRAVAR_LOG('ALTERACÃO','VENDA','VDA_OBS',:OLD.VDA_OBS,:NEW.VDA_OBS); 
            END IF;
    END;

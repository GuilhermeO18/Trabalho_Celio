            END IF;
        IF(:OLD.USU_CADASTRO != :NEW.USU_CADASTRO) THEN
            PROC_GRAVAR_LOG('ALTERACÃO','USUARIO','USU_CADASTRO',:OLD.USU_CADASTRO,:NEW.USU_CADASTRO); 

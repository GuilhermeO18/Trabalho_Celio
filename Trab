CREATE OR REPLACE TRIGGER TRG_UPDATE_PRODUTO 
    AFTER UPDATE ON  PRODUTO
    FOR EACH ROW
    BEGIN
        IF(:OLD.PRO_CODIGO != :NEW.PRO_CODIGO) THEN
            PROC_GRAVAR_LOG('ALTERACÃO','PRODUTO','PRO_CODIGO',:OLD.PRO_CODIGO,:NEW.PRO_CODIGO); 
            END IF;
        IF(:OLD.PRO_NOME != :NEW.PRO_NOME) THEN
            PROC_GRAVAR_LOG('ALTERACÃO','PRODUTO','PRO_NOME',:OLD.PRO_NOME,:NEW.PRO_NOME); 
            END IF;
        IF(:OLD.PRO_ESTOQUE != :NEW.PRO_ESTOQUE) THEN
            PROC_GRAVAR_LOG('ALTERACÃO','PRODUTO','PRO_ESTOQUE',:OLD.PRO_ESTOQUE,:NEW.PRO_ESTOQUE); 
            END IF;
        IF(:OLD.PRO_UNIDADE != :NEW.PRO_UNIDADE) THEN
            PROC_GRAVAR_LOG('ALTERACÃO','PRODUTO','PRO_UNIDADE',:OLD.PRO_UNIDADE,:NEW.PRO_UNIDADE); 
            END IF;
        IF(:OLD.PRO_PRECO != :NEW.PRO_PRECO) THEN
            PROC_GRAVAR_LOG('ALTERACÃO','PRODUTO','PRO_PRECO',:OLD.PRO_PRECO,:NEW.PRO_PRECO); 
            END IF;
        IF(:OLD.PRO_CUSTO != :NEW.PRO_CUSTO) THEN
            PROC_GRAVAR_LOG('ALTERACÃO','PRODUTO','PRO_CUSTO',:OLD.PRO_CUSTO,:NEW.PRO_CUSTO); 
            END IF;
        IF(:OLD.PRO_ATACADO != :NEW.PRO_ATACADO) THEN
            PROC_GRAVAR_LOG('ALTERACÃO','PRODUTO','PRO_ATACADO',:OLD.PRO_ATACADO,:NEW.PRO_ATACADO); 
            END IF;
        IF(:OLD.PRO_MIN != :NEW.PRO_MIN) THEN
            PROC_GRAVAR_LOG('ALTERACÃO','PRODUTO','PRO_MIN',:OLD.PRO_MIN,:NEW.PRO_MIN); 
            END IF;
        IF(:OLD.PRO_MAX != :NEW.PRO_MAX) THEN
            PROC_GRAVAR_LOG('ALTERACÃO','PRODUTO','PRO_MAX',:OLD.PRO_MAX,:NEW.PRO_MAX); 
            END IF;
        IF(:OLD.PRO_EMBALAGEM != :NEW.PRO_EMBALAGEM) THEN
            PROC_GRAVAR_LOG('ALTERACÃO','PRODUTO','PRO_EMBALAGEM',:OLD.PRO_EMBALAGEM,:NEW.PRO_EMBALAGEM); 
            END IF;
        IF(:OLD.PRO_PESO != :NEW.PRO_PESO) THEN
            PROC_GRAVAR_LOG('ALTERACÃO','PRODUTO','PRO_PESO',:OLD.PRO_PESO,:NEW.PRO_PESO); 
            END IF;
        IF(:OLD.PRO_CADASTRO != :NEW.PRO_CADASTRO) THEN
            PROC_GRAVAR_LOG('ALTERACÃO','PRODUTO','PRO_CADASTRO',:OLD.PRO_CADASTRO,:NEW.PRO_CADASTRO); 
            END IF;
        IF(:OLD.PRO_OBS != :NEW.PRO_OBS) THEN
            PROC_GRAVAR_LOG('ALTERACÃO','PRODUTO','PRO_OBS',:OLD.PRO_OBS,:NEW.PRO_OBS); 
            END IF;
        IF(:OLD.PRO_ATIVO != :NEW.PRO_ATIVO) THEN
            PROC_GRAVAR_LOG('ALTERACÃO','PRODUTO','PRO_ATIVO',:OLD.PRO_ATIVO,:NEW.PRO_ATIVO); 
            END IF;
    END;
    /

CREATE OR REPLACE TRIGGER TRG_UPDATE_USUARIO 
